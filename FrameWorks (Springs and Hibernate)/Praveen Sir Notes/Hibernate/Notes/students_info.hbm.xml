<?xml version="1.0" encoding = "UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
    
<hibernate-mapping>
	<class name="com.jspiders.hibernate.beans.StudentsInfoBean" table="students_info">
		<id name="regno" column="regno" />
		<property name="firstNM" 	column="firstname" />
		<property name="middleNM" column="middlename" />
		<property name="lastNM" 	column="lastname" />
	</class>
</hibernate-mapping>